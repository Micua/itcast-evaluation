{
  "name": "itcast-tms",
  "version": "4.0.0",
  "productName": "Itcast TMS",
  "description": "Teaching Management System by iceStone",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "cd src/core && npm install && cd ../updater && npm install && cd ../..",
    "clean:npm": "rm -rf node_modules && cd src/core && rm -rf node_modules && cd ../updater && rm -rf node_modules && cd ../..",
    "standard": "standard \"src/**/*.js\" \"updater/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" \"updater/**/*.js\"",
    "main-css": "cleancss -o src/core/static/style.min.css src/core/static/style.css",
    "predist": "npm run main-css",
    "dist": "gulp",
    "watch": "cross-env NODE_ENV=development gulp watch",
    "electron": "cross-env NODE_ENV=development electron ./src",
    "dev": "concurrently --raw --kill-others \"npm run watch\" \"npm run electron\"",
    "test": "cross-env NODE_ENV=development gulp test",
    "prestart": "npm run dist",
    "start": "cross-env NODE_ENV=production electron ./build",
    "juststart": "cross-env NODE_ENV=production electron ./build",
    "prepack": "npm run dist",
    "pack": "node packager"
  },
  "author": "iceStone <ice@wedn.net> (http://www.wedn.net/)",
  "license": "ISC",
  "keywords": [
    "iceStone",
    "Itcast",
    "TMS",
    "Teaching",
    "Electron"
  ],
  "devDependencies": {
    "angular": "latest",
    "angular-animate": "latest",
    "angular-route": "latest",
    "asar": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-es2015-loose": "latest",
    "bluebird": "latest",
    "clean-css": "latest",
    "concurrently": "latest",
    "cross-env": "latest",
    "del": "latest",
    "electron-packager": "latest",
    "electron-prebuilt": "latest",
    "eslint": "latest",
    "fs-extra": "latest",
    "gulp": "latest",
    "gulp-babel": "latest",
    "gulp-cssnano": "latest",
    "gulp-htmlmin": "latest",
    "gulp-if": "latest",
    "gulp-less": "latest",
    "gulp-livereload": "latest",
    "gulp-load-plugins": "latest",
    "gulp-rename": "latest",
    "gulp-size": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-uglify": "latest",
    "gulp-useref": "latest",
    "gulp-zip": "latest",
    "minimist": "latest",
    "normalize.css": "latest",
    "standard": "latest",
    "uglify-js": "latest"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Micua/itcast-tms.git"
  },
  "bugs": {
    "url": "https://github.com/Micua/itcast-tms/issues"
  },
  "homepage": "https://github.com/Micua/itcast-tms#readme",
  "build": {
    "app-bundle-id": "net.wedn.tms",
    "app-category-type": "net.wedn.tms",
    "iconUrl": "src/app.ico"
  }
}
