{
  "name": "itcast-tms",
  "version": "4.0.0",
  "productName": "Itcast TMS",
  "description": "Teaching Management System by iceStone",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd src && npm install && cd ../updater && npm install && cd ..",
    "clean:npm": "rm -rf node_modules && cd src && rm -rf node_modules && cd ../updater && rm -rf node_modules && cd ..",
    "standard":"standard \"src/**/*.js\" \"updater/**/*.js\"",
    "lint": "eslint src updater *.js",
    "main-css": "cleancss -o src/static/style.min.css src/static/style.css",
    "predist": "npm run main-css",
    "dist": "gulp",
    "watch": "cross-env NODE_ENV=development gulp watch",
    "electron": "cross-env NODE_ENV=development electron ./",
    "dev": "concurrently --raw --kill-others \"npm run watch\" \"npm run electron\"",
    "test": "cross-env NODE_ENV=development gulp test",
    "prestart": "npm run dist",
    "start": "cross-env NODE_ENV=production electron ./build",
    "prebuild": "echo '「build script」has been deprecated. Please use 「package script」 instead.' && gulp",
    "build": "npm run build:osx && npm run build:linux && npm run build:windows",
    "build:osx": "electron-packager . $npm_package_productName --overwrite --out=release --ignore='^/(node_modules|src|test)$' --prune --platform=darwin --arch=x64 --icon=src/app.icns --app-bundle-id=net.wedn.tms --app-version=$npm_package_version --version=$npm_package_electronVersion --overwrite",
    "build:mas": "electron-packager . $npm_package_productName --overwrite --out=release --ignore='^/(node_modules|src|test)$' --prune --platform=mas --arch=x64 --icon=src/app.icns --app-bundle-id=net.wedn.tms --app-version=$npm_package_version --version=$npm_package_electronVersion --overwrite",
    "build:linux": "electron-packager . $npm_package_productName --overwrite --out=release --ignore='^/(node_modules|src|test)$' --prune --platform=linux --arch=x64 --app-bundle-id=net.wedn.tms --app-version=$npm_package_version --version=$npm_package_electronVersion --overwrite",
    "build:windows": "electron-packager . $npm_package_productName --overwrite --out=release --ignore='^/(node_modules|src|test)$' --prune --platform=win32 --arch=ia32 --icon=src/app.ico --version=$npm_package_electronVersion --version-string.ProductName=$npm_package_productName --version-string.ProductVersion=$npm_package_electronVersion --overwrite",
    "prezip": "npm run dist",
    "zip": "gulp zip",
    "prepack": "npm run dist",
    "pack": "node packager"
  },
  "author": "iceStone <ice@wedn.net> (http://www.wedn.net/)",
  "license": "ISC",
  "keywords": [
    "iceStone",
    "Itcast",
    "TMS",
    "Teaching",
    "Electron"
  ],
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-route": "^1.5.5",
    "asar": "^0.11.0",
    "bluebird": "^3.3.5",
    "clean-css": "^3.4.12",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "del": "^2.2.0",
    "electron-connect": "^0.3.9",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^0.37.8",
    "eslint": "^2.8.0",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.2",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.1.0",
    "gulp-zip": "^3.2.0",
    "minimist": "^1.2.0",
    "normalize.css": "^4.1.1",
    "standard": "^7.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Micua/itcast-tms.git"
  },
  "bugs": {
    "url": "https://github.com/Micua/itcast-tms/issues"
  },
  "homepage": "https://github.com/Micua/itcast-tms#readme",
  "build": {
    "app-bundle-id": "net.wedn.tms",
    "app-category-type": "net.wedn.tms",
    "iconUrl": "src/app.ico"
  }
}
